    "files.exclude": {
        "**/.cache/": true
    },
    "search.exclude": {
        "**/build*/": true,
        "**/test-*/": true,
        "**/external/winfsp/tst/": true,
        "**/external/winfsp/tools/": true,
        "**/external/winfsp/doc/": true
    },
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.enhancedColorization": "enabled"
}